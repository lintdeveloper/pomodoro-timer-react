{
    "env": {
      "browser": true,
      "jest": true,
      "es6": true
    },
    "plugins": ["import"],
    "extends": [
       "eslint:recommended",
       "plugin:react/recommended"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "no-console": "off",
      "no-eval": "error",
      "import/first": "error"
    },
    "settings": {
        "import/extensions": [
          ".js",
          ".jsx"
        ]
    },
    "globals": {
        "process": true
    }
  }